import sys
from nrm.sharedlib import WrapEither, Lib
import nrm.daemon
import os

with Lib(os.environ["NRMSO"]) as lib:
    cfg = lib.cli(sys.argv[1:])
    nrm.daemon.runner(cfg, lib)
